<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meraj.shopmanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".createshop" />
        <activity android:name=".product1" />
        <activity android:name=".CreditDetails" />
        <activity android:name=".BuyProduct" />
        <activity android:name=".SellProduct" />
        <activity android:name=".Showing_All_Product" />
        <activity android:name=".EditProduct" />
        <activity android:name="com.example.meraj.shopmanagement.History"></activity>
    </application>

</manifest>